ECHO.
ECHO  visualstudio, vs	- Start Visual Studio
ECHO.
ECHO  sublime	-	Start Sublime Text
ECHO.
ECHO  ssms	-	Start Sql Server Management Studio	 
ECHO.
ECHO  sql	-	Start or Stop SQLExpress Service
ECHO.
ECHO  web -		Start or Stop Browser	
ECHO.
ECHO  skype	-	Start or Stop Skype	
ECHO.
ECHO  dropbox	-	Start or Stop Dropbox	
ECHO.
ECHO  chrome	-	Start or Stop Chrome
ECHO.
ECHO  vm	-	Start or Stop VirtuaBox
ECHO.
ECHO  kali	-	Start or Stop Kali
ECHO.
ECHO  notepad,  np	-	Start or Stop Notepad++
ECHO.
ECHO  filezilla,  ftp	-	Start or Stop Notepad++
ECHO.
